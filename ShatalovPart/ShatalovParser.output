Грамматика

    0 $accept: program $end

    1 program: statement_list

    2 statement_list: /* пусто */
    3               | statement_list statement

    4 statement: variable_declaration
    5          | expression_statement
    6          | block

    7 variable_declaration: type IDENTIFIER ';'
    8                     | type IDENTIFIER '=' expression ';'

    9 type: INT_TYPE
   10     | CHAR_TYPE
   11     | STRING_TYPE
   12     | BOOL_TYPE

   13 expression_statement: expression ';'

   14 block: '{' statement_list '}'

   15 expression: assignment_expression

   16 assignment_expression: logical_expression
   17                      | IDENTIFIER '=' expression

   18 logical_expression: comparison_expression

   19 comparison_expression: additive_expression
   20                      | comparison_expression EQUAL additive_expression
   21                      | comparison_expression NOT_EQUAL additive_expression
   22                      | comparison_expression '<' additive_expression
   23                      | comparison_expression '>' additive_expression
   24                      | comparison_expression LESS_EQUAL additive_expression
   25                      | comparison_expression GREATER_EQUAL additive_expression

   26 additive_expression: multiplicative_expression
   27                    | additive_expression '+' multiplicative_expression
   28                    | additive_expression '-' multiplicative_expression

   29 multiplicative_expression: primary_expression
   30                          | multiplicative_expression '*' primary_expression
   31                          | multiplicative_expression '/' primary_expression

   32 primary_expression: IDENTIFIER
   33                   | INTEGER_LITERAL
   34                   | CHAR_LITERAL
   35                   | STRING_LITERAL
   36                   | BOOL_LITERAL_TRUE
   37                   | BOOL_LITERAL_FALSE
   38                   | '(' expression ')'


